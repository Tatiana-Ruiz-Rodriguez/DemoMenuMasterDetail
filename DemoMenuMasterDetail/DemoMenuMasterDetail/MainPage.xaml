             xmlns:local="clr-namespace:DemoMenuMasterDetail"
             x:Class="DemoMenuMasterDetail.MainPage" 
             Padding="0,40,0,0"
             Icon="hamburger.png"
             Title="Ximian Icaza">

    <MasterDetailPage.Master>
        <local:MasterPage x:Name="masterPage"/>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Argument>
                <local:ContactosPage/>
            </x:Argument>
        </NavigationPage>
    </MasterDetailPage.Detail>   

</MasterDetailPage>
